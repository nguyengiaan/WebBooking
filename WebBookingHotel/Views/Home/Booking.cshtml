        }

            .owl-dots .owl-dot {
                display: none; /* Ẩn tất cả các chấm */
            }

                .owl-dots .owl-dot:nth-child(-n+4) {
                    display: inline-block; /* Hiển thị 4 chấm đầu tiên */
                    width: 10px; /* Kích thước của chấm */
                    height: 10px; /* Chiều cao của chấm */
                    margin: 0 5px; /* Khoảng cách giữa các chấm */
                    background: #333; /* Màu nền của chấm */
                    border-radius: 50%; /* Làm cho chấm có hình dạng tròn */
                    opacity: 0.6; /* Độ mờ của chấm */
                    transition: opacity 0.3s; /* Hiệu ứng chuyển tiếp khi thay đổi trạng thái */
                }

                .owl-dots .owl-dot.active {
                    background: #000; /* Màu nền của chấm khi được chọn */
                    opacity: 1; /* Độ mờ của chấm khi được chọn */
                }

    </style>

    <script>
        $(document).ready(function () {
            $("#uu-dai").owlCarousel({
                loop: true,
                margin: 10,
                nav: true,
                dots: false, // Tắt chấm điều hướng mặc định
                items: 3
            });

            // Điều chỉnh chấm điều hướng tùy chỉnh
            $("#uu-dai").on('changed.owl.carousel', function (event) {
                var index = event.item.index;
                $('#custom-dots .custom-dot').removeClass('active');
                $('#custom-dots .custom-dot').eq(index % 4).addClass('active');
            });
        });

        $(document).ready(function () {
            $("#uu-dai").owlCarousel({
                loop: true,
                margin: 10,
                nav: true,
                dots: true,
                items: 3, // Điều chỉnh số lượng item
                onInitialized: function () {
                    // Sau khi carousel được khởi tạo, loại bỏ chấm thừa
                    var dots = $('.owl-dot');
                    if (dots.length > 4) {
                        dots.slice(4).remove(); // Xóa các chấm từ thứ 5 trở đi
                    }
                }
            });
        });



